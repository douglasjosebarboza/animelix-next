datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String    @id @default(auto()) @map("_id") @db.ObjectId
  email     String    @unique
  password  String
  animeId   String[]
}

model Anime {
  id        String    @id @default(auto()) @map("_id") @db.ObjectId
  idFetch   String    @unique
  malId     String
  userId    String[]    
  episodes  String
}
